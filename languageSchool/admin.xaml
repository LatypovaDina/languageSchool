<Window x:Class="languageSchool.admin"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:languageSchool"
        mc:Ignorable="d"
        Title="Подтверждение администратора" Height="396" Width="322" MaxHeight="396" MaxWidth="322" MinHeight="396" MinWidth="322">
    <Grid>
        <TextBox x:Name="kod" HorizontalAlignment="Center" Height="25" Margin="0,146,0,0" VerticalAlignment="Top" Width="154"/>
        <Button x:Name="check" Content="Проверить" HorizontalAlignment="Center" Height="29" Margin="0,190,0,0" VerticalAlignment="Top" Width="114" Click="check_Click"/>
        <Button x:Name="back" Content="назад" Click="back_Click" HorizontalAlignment="Center" Height="20" Margin="0,224,0,0" VerticalAlignment="Top" Width="54"/>
        <Label Content="Введите код для активации администратора" HorizontalAlignment="Center" Height="34" Margin="0,107,0,0" VerticalAlignment="Top" Width="262"/>

    </Grid>
</Window>
